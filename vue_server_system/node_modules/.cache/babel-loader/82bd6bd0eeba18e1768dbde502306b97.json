veSessionStorage(key) {\r\n    sessionStorage.removeItem(key)\r\n}\r\n\r\n//向LocalStorage 存储一个JSON对象\r\nexport function setLocalStorage(key, value) {\r\n    localStorage.setItem(key, JSON.stringify(value))//JSON-转换-字符串\r\n}\r\n\r\n//取向LocalStorage中对象\r\nexport function getLocalStorage(key) {\r\n    var str = localStorage.getItem(key)\r\n    if (str == '' || str == null || str == 'null' || str == undefined) {\r\n        return null\r\n    } else {\r\n        return JSON.parse(str) //字符串转为 JSON对象\r\n    }\r\n}\r\n\r\n//删除LocalStorage中的对象\r\nexport function removeLocalStorage(key) {\r\n    localStorage.removeItem(key)\r\n}"],"mappings":"AAAA;AACA,OAAO,SAASA,UAAUA,CAAA,EAAG;EACzB,IAAIC,GAAG,GAAG,IAAIC,IAAI,EAAE;EACpB,IAAIC,IAAI,GAAGF,GAAG,CAACG,WAAW,EAAE;EAC5B,IAAIC,KAAK,GAAGJ,GAAG,CAACK,QAAQ,EAAE,GAAG,CAAC;EAC9B,IAAIC,GAAG,GAAGN,GAAG,CAACO,OAAO,EAAE;EACvBH,KAAK,GAAGA,KAAK,GAAG,EAAE,GAAG,GAAG,GAAGA,KAAK,GAAGA,KAAK;EACxCE,GAAG,GAAGA,GAAG,GAAG,EAAE,GAAG,GAAG,GAAGA,GAAG,GAAGA,GAAG;EAChC,OAAOJ,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG;AACzC;;AAEA;AACA;AACA;AACA,OAAO,SAASE,iBAAiBA,CAACC,GAAG,EAAEC,KAAK,EAAE;EAC1CC,cAAc,CAACC,OAAO,CAACH,GAAG,EAAEI,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC,CAAC;EACtD;AACA;;AAEA;AACA,OAAO,SAASK,iBAAiBA,CAACN,GAAG,EAAE;EACnC,IAAIO,GAAG,GAAGL,cAAc,CAACM,OAAO,CAACR,GAAG,CAAC;EACrC,IAAIO,GAAG,IAAI,EAAE,IAAIA,GAAG,IAAI,IAAI,IAAIA,GAAG,IAAI,MAAM,IAAIA,GAAG,IAAIE,SAAS,EAAE;IAC/D,OAAO,IAAI;EACf,CAAC,MAAM;IACH,OAAOL,IAAI,CAACM,KAAK,CAACH,GAAG,CAAC;EAC1B;AACJ;;AAEA;AACA,OAAO,SAASI,oBAAoBA,CAACX,GAAG,EAAE;EACtCE,cAAc,CAACU,UAAU,CAACZ,GAAG,CAAC;AAClC;;AAEA;AACA,OAAO,SAASa,eAAeA,CAACb,GAAG,EAAEC,KAAK,EAAE;EACxCa,YAAY,CAACX,OAAO,CAACH,GAAG,EAAEI,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC,CAAC;AACpD;;AAEA;AACA,OAAO,SAASc,eAAeA,CAACf,GAAG,EAAE;EACjC,IAAIO,GAAG,GAAGO,YAAY,CAACN,OAAO,CAACR,GAAG,CAAC;EACnC,IAAIO,GAAG,IAAI,EAAE,IAAIA,GAAG,IAAI,IAAI,IAAIA,GAAG,IAAI,MAAM,IAAIA,GAAG,IAAIE,SAAS,EAAE;IAC/D,OAAO,IAAI;EACf,CAAC,MAAM;IACH,OAAOL,IAAI,CAACM,KAAK,CAACH,GAAG,CAAC,EAAC;EAC3B;