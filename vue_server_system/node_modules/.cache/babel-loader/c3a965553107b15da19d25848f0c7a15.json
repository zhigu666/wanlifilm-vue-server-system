ocalStorage 存储一个JSON对象\r\nexport function setLocalStorage(key, value) {\r\n  localStorage.setItem(key, JSON.stringify(value)) //JSON-转换-字符串\r\n}\r\n\r\n//取向LocalStorage中对象\r\nexport function getLocalStorage(key) {\r\n  var str = localStorage.getItem(key)\r\n  if (str == '' || str == null || str == 'null' || str == undefined) {\r\n    return null\r\n  } else {\r\n    return JSON.parse(str) //字符串转为 JSON对象\r\n  }\r\n}\r\n\r\n//删除LocalStorage中的对象\r\nexport function removeLocalStorage(key) {\r\n  localStorage.removeItem(key)\r\n}\r\n"],"mappings":"AAAA;AACA,OAAO,SAASA,UAAUA,CAAA,EAAG;EAC3B,IAAIC,GAAG,GAAIA,GAAG,GAAG,IAAIC,IAAI,EAAG;EAE5B,IAAIC,IAAI,GAAGF,GAAG,CAACG,WAAW,EAAE;EAC5B,IAAIC,KAAK,GAAGJ,GAAG,CAACK,QAAQ,EAAE,GAAG,CAAC;EAC9B,IAAIC,GAAG,GAAGN,GAAG,CAACO,OAAO,EAAE;EAEvBH,KAAK,GAAGA,KAAK,GAAG,EAAE,GAAG,GAAG,GAAGA,KAAK,GAAGA,KAAK;EACxCE,GAAG,GAAGA,GAAG,GAAG,EAAE,GAAG,GAAG,GAAGA,GAAG,GAAGA,GAAG;EAChC,OAAOJ,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG;AACvC;;AAEA;AACA;;AAEA;AACA,OAAO,SAASE,iBAAiBA,CAACC,GAAG,EAAEC,KAAK,EAAE;EAC5CC,cAAc,CAACC,OAAO,CAACH,GAAG,EAAEI,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC,CAAC,EAAC;AACrD;;AAEA;AACA,OAAO,SAASK,iBAAiBA,CAACN,GAAG,EAAE;EACrC,IAAIO,GAAG,GAAGL,cAAc,CAACM,OAAO,CAACR,GAAG,CAAC,EAAC;EACtC,IAAIO,GAAG,IAAI,EAAE,IAAIA,GAAG,IAAI,IAAI,IAAIA,GAAG,IAAI,MAAM,IAAIA,GAAG,IAAIE,SAAS,EAAE;IACjE,OAAO,IAAI;EACb,CAAC,MAAM;IACL,OAAOL,IAAI,CAACM,KAAK,CAACH,GAAG,CAAC,EAAC;EACzB;AACF;;AAEA;AACA,OAAO,SAASI,oBAAoBA,CAACX,GAAG,EAAE;EACxCE,cAAc,CAACU,UAAU,CAACZ,GAAG,CAAC;AAChC;;AAEA;AACA,OAAO,SAASa,eAAeA,CAACb,GAAG,EAAEC,KAAK,EAAE;EAC1Ca,YAAY,CAACX,OAAO,CAACH,GAAG,EAAEI,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC,CAAC,EAAC;AACnD;;AAEA;AACA,OAAO,SAASc,eAAeA,CAACf,GAAG,EAAE;EACnC,IAAIO,GAAG,GAAGO,YAAY,CAACN,OAAO,CAACR,GAAG,CAAC;EACnC,IAAIO,GAAG,IAAI,EAAE,IAAIA,GAAG,IAAI,IAAI,IAAIA,GAAG,IAAI,MAAM,IAAIA,GAAG,IAAIE,SAAS,EAAE;IACjE,OAAO,IAAI;EACb,CAAC,MAAM;IACL,OAAOL,IAAI,CAACM,KAAK,CAACH,GAAG,CAAC,EAAC;EACzB;AACF;;AAEA;AACA,OAAO,SAASS,kBAAkBA,CAAChB,GAAG,EAAE;EACtCc,YAAY,CA